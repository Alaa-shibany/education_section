          { "name": "id", "type": "int" },
          { "name": "sessionDate", "type": "String", "json_key":"session_date" },
          { "name": "startTime", "type": "String" , "json_key":"start_time" },
          { "name": "endTime", "type": "String" , "json_key":"end_time" }
          
          ] }
      ]
    },
    {
